mixin menu (items, myClass)
    for item in items 
        li 
            a(data-filter=item.filter href=item.url class=myClass) #{item.name}

mixin aboutCard (item)
    figure
        figcaption
            .about-img
                picture 
                    source(srcset=item.src + ".webp", tipe="image/webp", data-src="lazy.svg")
                    img(src=item.src + ".png", alt=item.alt, data-src="lazy.svg")
                .employee-social.flex
                    a.fb(href="#") 
                    a.tw(href="#") 
                    a.gg(href="#")
                    a.in(href="#")
            .description-card._anim-items
                p.employee-name #{item.employeeName}
                p.description.roboto #{item.description}

