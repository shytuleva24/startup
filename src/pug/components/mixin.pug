mixin blogPost (item, myClass)
    .post
        .post-img.img-zoom
            img(src=item.src, alt=item.alt, data-src="lazy.svg")
        .post-info
            .data-name-post.flex 
                .data-post
                    p #{item.dataPost}
                    span #{item.monthPost}
                .name-post
                    p.text-left #{item.namePost}
                    i.text-left.roboto.sub-name-post By 
                        span Khalil 
                        | Uddin in 
                        span Development!
            .description-post
                p.text-info.roboto #{item.description}
                .read-more-content
                    p.text-info.roboto #{item.description}
                    p.text-info.roboto #{item.description}
                    p.text-info.roboto #{item.description}
                    p.text-info.roboto #{item.description}
            a.more-post.roboto(href="#popupBlog" class=myClass) Read more

mixin brand (items)
    for item in items 
        a.brand-company(href=item.url) 
            img(src=item.src, alt=item.alt)
            
